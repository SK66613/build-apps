{
  "id": "calendar_booking",
  "type": "htmlEmbed",
  "title": "Календарь: запись",
  "defaults": {
    "title": "Записаться на консультацию",
    "view": "month",
    "show_hold": true,
    "show_book": true,
    "show_contact": true,
    "contact_placeholder": "Ваш телефон/ник",
